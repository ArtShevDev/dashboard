<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs'); %>
<body>
    <header class="header">
        <div class="header__content _container">
            <a href="/" class="logo">logo</a>
            <%- include('./partials/nav.ejs'); %>
        </div>
    </header>
    <main class="page">
        <div class="page__content _container">
            <h1><%= title %></h1>
            <div class="page__data">
                <ul>
                    <% contact.forEach(({name, link}) => { %>
                        <li>
                            <span><%= name %></span>
                            <span><%= link %></span>
                        </li>
                    <% }) %>
                </ul>
            </div>
        </div>
    </main>
</body>
</html>